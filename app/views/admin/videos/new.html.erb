<div class="row">
  <div class="span7">
    <h1>Nové video</h1>
    <%= simple_form_for :video, url: admin_videos_path do |f| %>
      <%= f.input :title %>
      <%= f.simple_fields_for :link_attributes do |ff| %>
        <%= ff.input :href %>
      <% end %>
      <%= f.submit %>
    <% end %>
  </div>

  <div class="span4 well">
    <%= link_to 'Nápověda k Markdownu', 'https://github.com/adam-p/markdown-here/wiki/Markdown-Cheatsheet' %>
  </div>
</div>
